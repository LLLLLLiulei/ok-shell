{
  "name": "ok-shell",
  "productName": "OkShell",
  "version": "0.0.1",
  "description": "OkShell",
  "author": " ",
  "license": "ISC",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node script/build.js",
    "build:test": "cross-env NODE_ENV=production TEST=true node script/build.js",
    "build:all": "node .electron/build.js",
    "build:darwin": "cross-env NODE_ENV=production BUILD_TARGET=darwin node script/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node script/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node script/build.js",
    "build:win32": "cross-env NODE_ENV=production BUILD_TARGET=win32 node script/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node script/build.js",
    "dev": "cross-env NODE_ENV=development node script/dev-runner.js",
    "dev:web": "cross-env NODE_ENV=development BUILD_TARGET=web webpack-dev-server --inline --progress --config script/webpack.web.config.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src/",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config script/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config script/webpack.renderer.config.js",
    "analyze": "cross-env MODE=faster webpack --config script/webpack.analyze.config.js",
    "mem-2g": "cross-env LIMIT=2048 increase-memory-limit",
    "mem-3g": "cross-env LIMIT=3072 increase-memory-limit",
    "mem-4g": "cross-env LIMIT=4096 increase-memory-limit",
    "rebuild-pty": ".\\node_modules\\.bin\\electron-rebuild.cmd -f -w node-pty",
    "rebuild-pty:darwin": "./node_modules/.bin/electron-rebuild -f -w node-pty"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "element-plus": "^1.0.2-beta.35",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "lodash": "^4.17.15",
    "node-powershell": "^4.0.0",
    "node-pty": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "resize-detector": "^0.2.2",
    "ssh2": "^0.8.9",
    "ssh2-sftp-client": "^6.0.1",
    "uuid": "^3.3.3",
    "v-contextmenu": "^3.0.0-alpha.4",
    "vue": "^3.0.5",
    "vue-electron": "^1.0.6",
    "vue-router": "^4.0.3",
    "vue-runtime-helpers": "^1.1.2",
    "ws": "^7.4.4",
    "xterm": "^4.11.0",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
